{
    "name": "media-urls-updater",
    "version": "0.1.0",
    "description": "media urls updater",
    "main": "index.js",
    "engines": {
        "node": ">=20.0",
        "npm": ">=8.0"
    },
    "scripts": {
        "test": "exit 0 && echo \"Tests are not implemented yet\"",
        "build": "rm -rf dist && tsc",
        "dev": "npm run build && GOOGLE_APPLICATION_CREDENTIALS=key-file.json env-cmd npx @google-cloud/functions-framework --target=main --source=dist",
        "deploy": "npm run build && cp package*.json dist && gcloud functions deploy media-urls-updater --gen2 --trigger-http --runtime nodejs20 --memory=256Mb --entry-point=main --no-allow-unauthenticated --region=europe-central2 --source=dist --update-build-env-vars=\"GOOGLE_NODE_RUN_SCRIPTS=\" --project zinovik-project"
    },
    "dependencies": {
        "@google-cloud/functions-framework": "3.3.0",
        "@google-cloud/storage": "7.7.0",
        "axios": "1.6.7",
        "stream-shift": "1.0.3"
    },
    "devDependencies": {
        "@types/jest": "29.5.12",
        "env-cmd": "10.1.0",
        "eslint": "9.10.0",
        "jest": "29.7.0",
        "ts-jest": "29.1.2",
        "typescript": "5.3.3"
    },
    "author": "Max Zinovik",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zinovik/media-urls-updater.git"
    },
    "bugs": {
        "url": "https://github.com/zinovik/media-urls-updater/issues"
    },
    "homepage": "https://github.com/zinovik/media-urls-updater#readme"
}
